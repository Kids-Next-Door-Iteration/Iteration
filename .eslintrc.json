{
  "extends": ["airbnb", "airbnb/hooks", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": 2,
    "indent": [2, 2],
    "no-unused-vars": ["off", { "vars": "local" }],
    "prefer-const": 2,
    "quotes": [2, "single"],
    "react/prop-types": "off",
    "semi": [2, "always"],
    "space-infix-ops": 2,
    "no-empty": 2,
    "no-empty-function": 2,
    "no-console": "off",
    "import/no-unresolved": false
  }
}
